#include<iostream>

using namespace std;

int hourglassSum(int arr[6][6])
{
    
    int max_sum = 0;
    for (int i=0; i<4; i++)
    {
        for (int j=0; j<4; j++)
        {
            int sum = (arr[i][j]+arr[i][j+1]+arr[i][j+2])+
                      (arr[i+1][j+1])+
                  (arr[i+2][j]+arr[i+2][j+1]+arr[i+2][j+2]);
  

            max_sum = max(max_sum, sum);
            if(sum>max_sum)
            {
                max_sum = sum;
            }
        }
    }
    return max_sum;
}

int main()
{
    int i,j,arr[6][6];
    for(i=0;i<6;i++)
    {
        for(j=0;j<6;j++)
        {
            cin>>arr[i][j];
        }
    }
    
    int result = hourglassSum(arr);
    cout<<result;
}
